{
    "name":"bscheshirwork/yii2-cubs",
    "description":"Trait for AR. Include [create|update][At|By], flags and blockAt fields",
    "keywords":["AR", "Db", "Yii2", "ActiveRecord", "ActiveRecord", "migration", "gii template"],
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"BSCheshir",
            "email":"bscheshirwork@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "bscheshirwork\\cubs\\": "src\\"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "@dev"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
